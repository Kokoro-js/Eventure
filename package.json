{
  "name": "eventure",
  "version": "0.3.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "event",
    "hook",
    "emit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kokoro-js/eventure.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "dev": "bun run --hot .src\\index.ts",
    "test": "bun test",
    "build": "tsdown",
    "prepublish": "tsdown",
    "lint": "biome lint",
    "format:check": "biome format",
    "format": "biome format --write",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/bun": "^1.3.2",
    "tsdown": "^0.16.1",
    "type-fest": "^5.2.0",
    "typescript": "^5.9.3"
  }
}
